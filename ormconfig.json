{
    "type": "sqlite",
    "database": "db.sqlite3",
    "autoSchemaSync": true,
    "synchronize": true,
    "loggin": true,
    "entities": [
        "src/entity/**/*.ts"
    ],
    "migrations": [
        "src/migration/**/*.ts"
    ],
    "subscribers": [
        "src/subscriber/**/*.ts"
    ],
    "cli": {
        "entitiesDir": "src/entity",
        "migrationsDir": "src/migration",
        "subscriberDir": "src/subscriber"
    }
}

